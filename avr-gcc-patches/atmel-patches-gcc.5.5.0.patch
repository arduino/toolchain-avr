+
+  if (v)
+    FOR_EACH_VEC_ELT (*v, i, opt)
+      {
+				switch (opt->opt_index)
+					{
+					case OPT_mnon_bit_addressable_registers_mask_:
+						avr_non_bit_addressable_registers_mask = parse_unsigned_long (opt->arg);
+						break;
+
+					default:
+						gcc_unreachable ();
+					}
+			}
+}
 
